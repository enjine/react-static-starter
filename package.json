{
  "name": "react-static-starter",
  "version": "1.0.0",
  "description": "Barebones React scaffold without using create-react-app. SCSS for styles. Parcel as the bundler.",
  "keywords": [],
  "author": "Alessandro Di Candia <alex@engine.net>",
  "license": "MIT",
  "main": "index.js",
  "dependencies": {
    "minimal-css-reset": "^2.0.0",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "acorn": "^7.1.1",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^4.2.3",
    "node-sass": "^7.0.0",
    "parcel-bundler": "^1.12.4",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "scripts": {
    "dev": "parcel $npm_package_parcel_entrypoints -p 3000",
    "start": "parcel $npm_package_parcel_entrypoints",
    "prebuild": "rm -rf dist/",
    "build": "parcel build $npm_package_parcel_entrypoints",
    "lint": "eslint ./src/ --fix"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": true,
      "postcss-flexbugs-fixes": true
    }
  },
  "parcel": {
    "entrypoints": "src/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
